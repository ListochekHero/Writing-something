#include <string>
#include <iostream>
using namespace std;
std::string correct(std::string str){
    for(int i=0;i<str.length();++i){
      if(str.find('5')!=str.npos){
        str.replace(str.find('5'),1,"S");
      }
      if(str.find('0')!=str.npos){
        str.replace(str.find('0'),1,"O");
      }
      if(str.find('1')!=str.npos){
        str.replace(str.find('1'),1,"I");
      }
    }
  return str;
}
